{
	"name": "oops/morozko",
	"description": "Cache warmup library for Nette Framework.",
	"keywords": ["nette", "cache", "warmup"],
	"type": "library",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jiří Pudil",
			"email": "me@jiripudil.cz",
			"homepage": "https://jiripudil.cz"
		}
	],
	"support": {
		"email": "me@jiripudil.cz",
		"issues": "https://github.com/o2ps/Morozko/issues"
	},
	"require": {
		"php": ">= 7.1.0",
		"nette/di": "^2.4.10",
		"nette/finder": "^2.4",
		"ocramius/package-versions": "^1.1",
		"symfony/console": "^2.3||^3.0",
		"symfony/process": "^3.3"
	},
	"require-dev": {
		"kdyby/console": "^2.7.0",
		"mockery/mockery": "^1.0.0",
		"nette/bootstrap": "^2.4.0",
		"nette/tester": "^2.0.0"
	},
	"autoload": {
		"psr-4": {
			"Oops\\Morozko\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"OopsTests\\Morozko\\": "tests/Morozko"
		}
	}
}
